<h1>New Item</h1>

<%= form_tag(items_url, method: 'post') do %>

  <div>
    <%= label_tag :title %><br />
    <%= text_field_tag :title %>
  </div>

    <div>
    <%= label_tag :address %><br />
    <%= text_field_tag :address, "", size: 50 %>
  </div>


  <div>
    <%= submit_tag "Create Item" %>
  </div>
<% end %>
